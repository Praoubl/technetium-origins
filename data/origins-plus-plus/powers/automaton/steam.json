{
	"description":"You are powered by Steam, generated from water by Fire.",
	"type":"multiple",
	"bar":{
		"type":"simple"
	},
	"generate":{
		"type":"origins:action_over_time",
		"interval":1,
		"actions": [
			{
				"type": "origins:feed",
				"food": 2,
				"saturation": 0
			},
			{
				"type":"origins:execute_command",
				"command":"resource change @s origins-plus-plus:automaton/firebar -100"
			},
			{
				"type":"origins:execute_command",
				"command":"resource change @s origins-plus-plus:automaton/waterbar -100"
			}
		],
		"condition":{
			"type": "origins:food_level",
			"comparison": "<",
			"compare_to": 20
		}
	}
}